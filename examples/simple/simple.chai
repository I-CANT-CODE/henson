var procmap = ProcMap()

if(procmap.group("world"))
{
    var sim = load("sim", "../simple/simulation 250", procmap)
    var ana = load("ana", "../simple/analysis", procmap)

    sim.proceed()

    while(sim.running())
    {
        ana.proceed()
        sim.proceed()
    }
}
else
{
    print("I don't belong to group world!!")
}
